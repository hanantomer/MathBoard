{
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts" },
    "**/**.js": { "when": "$(basename).tsx" }
  },
  "eslint.workingDirectories": [
    "./{PATH_TO_CLIENT}"
  ],
    "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "server",
          "commands": ["cd math-server","npm run dev"]
        },
      ]
    },
  ]
}